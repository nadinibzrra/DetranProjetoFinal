<AnchorPane id="AnchorPane" fx:id="anchorProva" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controle.TelaProvaController">
   <children>
      <JFXDatePicker fx:id="campoData" layoutX="95.0" layoutY="140.0" promptText="Data Da Prova" />
